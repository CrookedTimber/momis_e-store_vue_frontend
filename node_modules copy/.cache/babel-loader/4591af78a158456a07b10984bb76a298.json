{"ast":null,"code":"export default {\n  name: 'Checkout',\n  data() {\n    return {\n      cart: {\n        items: []\n      },\n      stripe: {},\n      card: {},\n      first_name: '',\n      last_name: '',\n      email: '',\n      phone: '',\n      address: '',\n      zipcode: '',\n      place: '',\n      errors: []\n    };\n  },\n  mounted() {\n    document.title = 'Checkout | Momis';\n    this.cart = this.$store.state.cart;\n  },\n  methods: {\n    getItemTotal(item) {\n      return item.quantity * item.product.price;\n    },\n    submitForm() {}\n  },\n  computed: {\n    cartTotalLength() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return acc += curVal.quantity;\n      }, 0);\n    },\n    cartTotalPrice() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return acc += curVal.product.price * curVal.quantity;\n      }, 0);\n    }\n  }\n};","map":{"version":3,"mappings":"AAkHA,eAAe;EACbA,IAAI,EAAE,UAAU;EAEhBC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT,CAAC;MACDC,MAAM,EAAE,CAAC,CAAC;MACVC,IAAI,EAAE,CAAC,CAAC;MACRC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACRC,QAAQ,CAACC,KAAI,GAAI,kBAAkB;IACnC,IAAI,CAACd,IAAG,GAAI,IAAI,CAACe,MAAM,CAACC,KAAK,CAAChB,IAAI;EACpC,CAAC;EACDiB,OAAO,EAAE;IACPC,YAAY,CAACC,IAAI,EAAE;MACjB,OAAOA,IAAI,CAACC,QAAO,GAAID,IAAI,CAACE,OAAO,CAACC,KAAK;IAC3C,CAAC;IACDC,UAAU,GAAG,CAAC;EAChB,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAe,GAAG;MAChB,OAAO,IAAI,CAACzB,IAAI,CAACC,KAAK,CAACyB,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;QAC7C,OAAQD,GAAE,IAAKC,MAAM,CAACR,QAAQ;MAChC,CAAC,EAAE,CAAC,CAAC;IACP,CAAC;IACDS,cAAc,GAAG;MACf,OAAO,IAAI,CAAC7B,IAAI,CAACC,KAAK,CAACyB,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;QAC7C,OAAQD,GAAE,IAAKC,MAAM,CAACP,OAAO,CAACC,KAAI,GAAIM,MAAM,CAACR,QAAQ;MACvD,CAAC,EAAE,CAAC,CAAC;IACP;EACF;AACF,CAAC","names":["name","data","cart","items","stripe","card","