{"ast":null,"code":"import axios from 'axios';\nimport { toast } from 'bulma-toast';\nimport CartItem from '@/components/CartItem.vue';\nexport default {\n  name: 'Cart',\n  components: {\n    CartItem\n  },\n  data() {\n    return {\n      cart: {\n        items: []\n      }\n    };\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n    console.log(this.cart);\n  },\n  computed: {\n    cartTotalLength() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return acc += curVal.quantity;\n      }, 0);\n    },\n    cartTotalPrice() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return acc += curVal.product.price * curVal.quantity;\n      }, 0);\n    }\n  }\n};","map":{"version":3,"mappings":"AA6CA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,KAAI,QAAS,aAAa;AACnC,OAAOC,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,MAAM,CAACC,KAAK,CAACJ,IAAI;IAClCK,OAAO,CAACC,GAAG,CAAC,IAAI,CAACN,IAAI,CAAC;EACxB,CAAC;EACDO,QAAQ,EAAE;IACRC,eAAe,GAAG;MAChB,OAAO,IAAI,CAACR,IAAI,CAACC,KAAK,CAACQ,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;QAC7C,OAAQD,GAAE,IAAKC,MAAM,CAACC,QAAQ;MAChC,CAAC,EAAE,CAAC,CAAC;IACP,CAAC;IACDC,cAAc,GAAG;MACf,OAAO,IAAI,CAACb,IAAI,CAACC,KAAK,CAACQ,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;QAC7C,OAAQD,GAAE,IAAKC,MAAM,CAACG,OAAO,CAACC,KAAI,GAAIJ,MAAM,CAACC,QAAQ;MACvD,CAAC,EAAE,CAAC,CAAC;IACP;EACF;AACF,CAAC","names":["axios","toast","CartItem","name","components","data","cart","items","mounted","$store","state","console","log","computed","cartTotalLength","reduce","acc","curVal","quantity","cartTotalPrice","product","price"],"sourceRoot":"","sources":["C:\\Edgar\\Web Development\\00_job_seeking\\portfolio\\E-Commerce\\momis_vue_eshop\\src\\views\\CartView.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-cart\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">Cart</h1>\r\n      </div>\r\n\r\n      <div class=\"column is-12 box\">\r\n        <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\r\n          <thead>\r\n            <tr>\r\n              <th>Product</th>\r\n              <th>Price</th>\r\n              <th>Quantity</th>\r\n              <th>Total</th>\r\n              <th></th>\r\n            </tr>\r\n          </thead>\r\n\r\n          <tbody>\r\n            <CartItem\r\n              v-for=\"item in cart.items\"\r\n              v-bind:key=\"item.product.id\"\r\n              v-bind:initialItem=\"item\"\r\n            />\r\n          </tbody>\r\n        </table>\r\n        <p v-else>There are no products in your cart.</p>\r\n      </div>\r\n      <div class=\"column is-12 box\">\r\n        <h2 class=\"subtitle\">Summary</h2>\r\n        <strong>${{ cartTotalPrice.toFixed(2) }}</strong>, {{ cartTotalLength }} items\r\n\r\n\r\n        <hr>\r\n\r\n        <router-link to=\"/cart/checkout\" class=\"button is-dark\"\r\n          >Checkout</router-link\r\n        >\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { toast } from 'bulma-toast';\r\nimport CartItem from '@/components/CartItem.vue';\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  components: {\r\n    CartItem,\r\n  },\r\n  data() {\r\n    return {\r\n      cart: {\r\n        items: [],\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.cart = this.$store.state.cart;\r\n    console.log(this.cart);\r\n  },\r\n  computed: {\r\n    cartTotalLength() {\r\n      return this.cart.items.reduce((acc, curVal) => {\r\n        return (acc += curVal.quantity);\r\n      }, 0);\r\n    },\r\n    cartTotalPrice() {\r\n      return this.cart.items.reduce((acc, curVal) => {\r\n        return (acc += curVal.product.price * curVal.quantity);\r\n      }, 0);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}