{"ast":null,"code":"import axios from 'axios';\nimport { toast } from 'bulma-toast';\nexport default {\n  name: 'Category',\n  data() {\n    return {\n      category: {\n        products: []\n      }\n    };\n  },\n  mounted() {\n    this.getCategory();\n  },\n  methods: {\n    async getCategory() {\n      this.$store.commit('setIsLoading', true);\n      const category_slug = this.$route.params.category_slug;\n      await axios.get(`/api/v1/products/${category_slug}/`).then(response => {\n        this.category = response.data;\n        document.title = `${this.category.name} | Momis`;\n      }).catch(error => {\n        console.log(error);\n      });\n      toast({\n        message: 'Something went wrong. Please try again',\n        type: 'is-danger',\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n      this.$store.commit('setIsLoading', false);\n    },\n    addToCart() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n      const item = {\n        product: this.product,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', item);\n      toast({\n        message: 'Product added to the cart',\n        type: 'is-success',\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,KAAI,QAAS,aAAa;AACnC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QAAEC,QAAQ,EAAE;MAAG;IAC3B,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,WAAW,EAAE;EACpB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAW,GAAG;MAClB,IAAI,CAACE,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI,CAAC;MACxC,MAAMC,aAAY,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,aAAa;MAEtD,MAAMX,KAAI,CACPc,GAAG,CAAE,oBAAmBH,aAAc,GAAE,EACxCI,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACZ,QAAO,GAAIY,QAAQ,CAACb,IAAI;QAC7Bc,QAAQ,CAACC,KAAI,GAAK,GA