{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Product',\n  data() {\n    return {\n      product: {},\n      quantity: 1\n    };\n  },\n  mounted() {\n    this.getProduct();\n  },\n  methods: {\n    getProduct() {\n      const category_slug = this.$route.params.category_slug;\n      const product_slug = this.$route.params.product_slug;\n      axios.get(`/api/v1/products/${category_slug}/${product_slug}`).then(response => {\n        this.product = response.data;\n      }).catch(error => console.log(error));\n    },\n    addToCart() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n      const item = {\n        product: this.product,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', item);\n      toast({\n        message: 'Product added to the cart',\n        type: \"is_success\",\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,CAAC;MACXC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,UAAU,EAAE;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,UAAU,GAAG;MACX,MAAME,aAAY,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,aAAa;MACtD,MAAMG,YAAW,GAAI,IAAI,CAACF,MAAM,CAACC,MAAM,CAACC,YAAY;MAEpDX,KAAI,CACDY,GAAG,CAAE,oBAAmBJ,aAAc,IAAGG,YAAa,EAAC,EACvDE,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACX,OAAM,GAAIW,QAAQ,CAACZ,IAAI;MAC9B,CAAC,EACAa,KAAK,CAAEC,KAAK,IAAKC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC,CAAC;IACzC,CAAC;IACDG,SAAS,GAAG;MACV,IAAIC,KAAK,CAAC,IAAI,CAAChB,QAAQ,KAAK,IAAI,CAACA,QAAO,GAAI,CAAC,EAAE;QAC7C,IAAI,CAACA,QAAO,GAAI,CAAC;MACnB;MACA,MAAMiB,IAAG,GAAI;QACXlB,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MAED,IAAI,CAACkB,MAAM,CAACC,MAAM,CAAC,WAAW,EAAEF,IAAI,CAAC;MAErCG,KAAK,CAAC;QACJC,OAAO,EAAE,2BAA2B;QACpCC,IAAI,EAAE,YAAY;QAClBC,WAAW,EAAE,I